<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Alias</th>
      <th>Project Title</th>
      <th>project_id</th>
      <th># of samples</th>
      <th>Description</th>
      <th>Publication</th>
      <th>Expermien</th>
      <th>Analysis</th>
      <th>Client</th>
      <th>Collector</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.alias %></td>
        <td><%= link_to project.title, project_examinations_path(project.id)  %></td>

        <td><%= project.id %></td>
        <td><%= project.id %></td>

        <td><%= project.description %></td>
        <td><%= project.publication %></td>
        <td><%= project.expermien_id %></td>
        <td><%= project.analysis_id %></td>
        <td><%= project.client_id %></td>
        <td><%= project.collector_id %></td>
        <td><%= project.user_id %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= form_tag({controller: "projects", action: "import", method: "post"}, {multipart: true}) do %>
  <%= file_field_tag :file %>
  <%= submit_tag "データを一括インポート" %>
<% end %>
<br>

<%= link_to 'New Project', new_project_path %>
